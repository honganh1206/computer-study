{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Active File",
      "type": "shell",
      "command": "/usr/bin/gcc-11",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"],
      "detail": "Generated task to build active C file"
    }
  ]
}
